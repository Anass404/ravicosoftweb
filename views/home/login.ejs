                <div class="banner_inner_text">
                    <h2>Login</h2>
                    <!-- <p>Login To Continue</p> -->
                </div>
            </div>
        </section>
        <!--================End Banner Area =================-->
        
        <!--================Get in Touch Area =================-->
        <section class="">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 ">
                        <form class="row" action="/auth/signin" method="post">
                            <div class="form-group col-lg-6">
                                <input type="text" class="form-control" id="email" name="username" placeholder="Email">
                            </div>
                            <div class="form-group col-lg-6">
                                <input type="password" class="form-control" id="password" name="password" placeholder="Password">
                            </div>
                            <div class="form-group col-lg-12">
                                <button type="submit" value="submit" class="btn submit_btn form-control" style="line-height: unset; height: 50px;">Login</button>
                            </div>
                        </form>
                    </div>
                    
                </div>
            </div>
        </section>
